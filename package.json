{
  "name": "trade",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=16.18.0 <17.0.0"
  },
  "scripts": {
    "start": "vite",
    "start:stg": "vite --mode staging",
    "start:prod": "vite --mode production",
    "serve": "vite preview",
    "build": "tsc && vite build",
    "build:dev": "cp .env.development .env && tsc && vite build --mode development",
    "build:staging": "cp .env.staging .env && tsc && vite build --mode staging",
    "build:prod": "cp .env.production .env && tsc && vite build --mode production",
    "preview": "vite preview --port 8080",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "eslint --fix \"src/**/*.*\"",
    "fix:prettier": "prettier --write \"src/**/*.*\"",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"src/**/*.*\"",
    "lint:prettier": "prettier --check \"src/**/*.*\"",
    "i18n:extract": "lingui extract",
    "i18n:extract-clean": "lingui extract --clean",
    "i18n:compile": "lingui compile",
    "i18n": "yarn i18n:extract && yarn i18n:compile",
    "pre-commit": "lint-staged --allow-empty",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
